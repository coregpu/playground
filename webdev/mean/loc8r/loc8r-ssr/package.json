{
  "name": "loc8r-ssr",
  "version": "1.0.0",
  "description": "A server-side rendered app that shows places with good wifi where you can get some work done.",
  "main": "dist/app.js",
  "scripts": {
    "start": "node -r dotenv-safe/config.js dist/app.js",
    "dev:start": "nodemon -r dotenv-safe/config.js -w dist/app.js -w dist/app_server -e js",
    "build": "gulp",
    "dev:build": "gulp buildAndWatch",
    "dev:buildAndStart": "run-p dev:build dev:start",
    "lint": "gulp lint",
    "clean": "rimraf dist/*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Loc8r",
    "MEAN",
    "Express",
    "Angular",
    "Node",
    "Mongo"
  ],
  "author": "Dzanan Bajgoric",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "bootstrap": "^4.6.1",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "dotenv-safe": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "files-exist": "^1.1.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^6.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^5.0.1",
    "merge-stream": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.43.4",
    "typescript": "^4.4.4"
  }
}
